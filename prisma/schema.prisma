   datasource db {
     provider = "mongodb"
     url      = env("DATABASE_URL")
   }

   generator client {
     provider = "prisma-client-js"
   }

   model Space {
  id          String   @id @default(auto()) @map("_id") @db.ObjectId
  name        String
  header      String
  description String?
  slug        String @unique
  createdAt   DateTime @default(now())
}